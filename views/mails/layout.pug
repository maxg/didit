doctype strict
html
  head
    block head
      style(type='text/css').
        body {
          color: #333;
          background-color: #fff;
        }
        a {
          color: #337ab7;
          text-decoration: none;
        }
        a:hover {
          color: #23527c;
          text-decoration: underline;
        }
        p {
          margin: 0 0 10px;
        }
        h1, h2, h3, h4, h5, h6 {
          margin: 10px 0;
          font-family: inherit;
          font-weight: bold;
          color: inherit;
        }
        ul,
        ol {
          padding: 0;
          margin: 0 0 10px 25px;
        }
        ul ul,
        ul ol,
        ol ol,
        ol ul {
          margin-bottom: 0;
        }
        li {
          margin-bottom: 4px;
        }
        ul.unstyled,
        ol.unstyled {
          margin-left: 0;
          list-style: none;
        }
        hr {
          margin: 20px 0;
          border: 0;
          border-top: 1px solid #eee;
        }
        tt {
          font-family: Menlo, Consolas, "Courier New", monospace;
        }
        pre {
          display: block;
          padding: 9.5px;
          margin: 0 0 10px;
          color: #333;
          word-break: break-all;
          word-wrap: break-word;
          white-space: pre;
          white-space: pre-wrap;
          background-color: #f5f5f5;
          border: 1px solid #ccc;
          -webkit-border-radius: 4px;
          -moz-border-radius: 4px;
          border-radius: 4px;
        }
        .label {
          display: inline-block;
          padding: 1px 2px;
          font-size: 11px;
          font-weight: bold;
          line-height: 13px;
          color: #fff;
          text-align: center;
          white-space: nowrap;
          vertical-align: baseline;
          -webkit-border-radius: .25em;
          -moz-border-radius: .25em;
          border-radius: .25em;
        }
        .label {
          -webkit-border-radius: 3px;
          -moz-border-radius: 3px;
          border-radius: 3px;
        }
        .label:empty {
          display: none;
        }
        .label-default {
          background-color: #777;
        }
        .label-primary {
          background-color: #337ab7;
        }
        .label-success {
          background-color: #5cb85c;
        }
        .label-info {
          background-color: #5bc0de;
        }
        .label-warning {
          background-color: #f0ad4e;
        }
        .label-danger {
          background-color: #d9534f;
        }
        .text-muted {
          color: #777;
        }
        .alert {
          padding: 15px;
          margin-bottom: 20px;
          border: 1px solid transparent;
          -webkit-border-radius: 4px;
          -moz-border-radius: 4px;
          border-radius: 4px;
        }
        .alert-success {
          color: #3c763d;
          background-color: #dff0d8;
          border-color: #d6e9c6;
        }
        .alert-info {
          color: #31708f;
          background-color: #d9edf7;
          border-color: #bce8f1;
        }
        .alert-warning {
          color: #8a6d3b;
          background-color: #fcf8e3;
          border-color: #faebcc;
        }
        .alert-danger {
          color: #a94442;
          background-color: #f2dede;
          border-color: #ebccd1;
        }
        ul.labeled {
          margin-left: 24px;
          list-style: none;
        }
        ul.labeled li {
          margin-left: 0;
        }
        ul.labeled .label {
          margin-right: 4px; /* Gmail does not support negative margin */
          margin: 0 10px 0 -24px;
          width: 10px;
          text-align: center;
        }
        .alert > h1:first-child,
        .alert > h2:first-child,
        .alert > h3:first-child,
        .alert > h4:first-child {
          margin-top: 0;
        }
        .alert > h1:last-child,
        .alert > h2:last-child,
        .alert > h3:last-child,
        .alert > h4:last-child,
        .alert > p:last-child,
        .alert > ul:last-child {
          margin-bottom: 0;
        }
  body
    block content
    hr
    p.text-muted The Classroom Continuous Build Butler - Didit
    p.text-muted If you received this message in error, please contact&nbsp;
      a(href=`mailto:${config.mail.owner}`)= config.mail.owner
      | .
